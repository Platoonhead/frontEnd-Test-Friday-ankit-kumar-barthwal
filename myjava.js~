$.ajax({  
		      type: "GET",  
		      url: "https://public-api.wordpress.com/rest/v1/sites/dotnetask.wordpress.com",
		      success: function(value) {  

		          $('#q').html(value.name);
		          $('#r').html(value.ID);
		          $('#s').html(value.description);

		      }
	      });
